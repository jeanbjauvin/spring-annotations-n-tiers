{
    "configuration": {
        "status": "warn",
        "appenders": {
            "Console": {
                "name": "console",
                "target": "SYSTEM_OUT",
                "PatternLayout": {
                    "pattern": "%d{HH:mm:ss:SSS} [%t] %-5level %logger{36} - %msg%n"
                }
            }
        },
        "loggers": {
            "Root": {
                "level": "debug",
                "AppenderRef": {
                    "ref": "console"
                }
            }
        }
    }
}